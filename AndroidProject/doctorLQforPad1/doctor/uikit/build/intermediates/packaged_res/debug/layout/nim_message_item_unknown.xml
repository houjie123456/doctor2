<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/message_item_unsupport_container"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/nim_message_item_round_bg"
    android:paddingTop="10dp"
    android:paddingBottom="10dp">

    <!--<ImageView-->
        <!--android:id="@+id/message_item_unsupport_image"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_centerVertical="true"-->
        <!--android:layout_marginLeft="10dp"-->
        <!--android:contentDescription="@string/empty"-->
        <!--android:src="@drawable/nim_unsupport_mime_type"/>-->

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="6dip"
        android:layout_marginRight="4dip"
        android:orientation="vertical">
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/pat_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:gravity="left"
        android:singleLine="true"
        android:text="姓名"
        android:textColor="@color/color_black_333333"
        android:textSize="16sp"/>
    <TextView
        android:id="@+id/pat_sex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:gravity="left"
        android:singleLine="true"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="年龄"
        android:textColor="@color/color_black_333333"
        android:textSize="16sp"/>
    <TextView
        android:id="@+id/pat_age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:gravity="left"
        android:singleLine="true"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="岁数"
        android:textColor="@color/color_black_333333"
        android:textSize="16sp"/>
</LinearLayout>


        <TextView
            android:id="@+id/pat_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dip"
            android:ellipsize="end"
            android:gravity="left"
            android:singleLine="true"
            android:text="@string/unsupport_desc"
            android:textColor="@color/color_grey_555555"
            android:textSize="13sp"/>
    </LinearLayout>
</RelativeLayout>